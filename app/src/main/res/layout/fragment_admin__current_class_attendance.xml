<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:id="@+id/attendance_admin_rl"
    tools:context="com.quantumsit.sportsinc.ADMINS.Admin_CurrentClassAttendanceFragment">

    <!--ImageView
        android:id="@+id/checkedImageView_coachcurrentclassattendancefragment"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:src="@drawable/ic_check_faded" /-->

    <TextView
        android:id="@+id/textView_admincurrentclassattendancefragment"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textColor="@color/colorTextDark"
        android:textStyle="bold"
        android:layout_margin="16dp"
        android:text="@string/attendance" />

    <ListView
        android:id="@+id/rulesCheckListView_admincurrentclassattendancefragment"
        android:layout_below="@+id/textView_admincurrentclassattendancefragment"
        android:layout_marginStart="20dp"
        android:layout_marginLeft="20dp"
        android:layout_width="match_parent"
        android:layout_height="wrap_content" >


    </ListView>

    <android.support.design.widget.FloatingActionButton
        android:id="@+id/doneFloatingActionButton_admincurrentclassattendancefragment"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:clickable="true"
        android:focusable="true"
        android:layout_margin="16dp"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:src="@drawable/ic_arrow_forward" />

</RelativeLayout>
